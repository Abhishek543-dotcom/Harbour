# General Environment Variables
PROJECT_NAME=harbour

# Spark Configuration
SPARK_MASTER_URL=spark://spark-master:7077
SPARK_DRIVER_MEMORY=2g
SPARK_EXECUTOR_MEMORY=4g
SPARK_EXECUTOR_CORES=2

# Jupyter Configuration
JUPYTER_PASSWORD=yourpassword
JUPYTER_PORT=8888
JUPYTER_BASE_URL=http://localhost:8888

# MinIO Configuration
MINIO_ACCESS_KEY=youraccesskey
MINIO_SECRET_KEY=yoursecretkey
MINIO_BUCKET_NAME=delta-bucket
MINIO_PORT=9000
MINIO_URL=http://localhost:9000

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=yourpostgrespassword
POSTGRES_DB=delta_metadata
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Delta Lake Configuration
DELTA_LAKE_PATH=/mnt/delta
DELTA_STORAGE_URL=http://localhost:9000/delta-bucket

# Kubernetes Configuration (if needed)
K8S_NAMESPACE=default
K8S_CLUSTER_URL=https://<kubernetes-cluster-url>
K8S_TOKEN=<your-k8s-token>
